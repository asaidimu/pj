#compdef pj

_pj(){
  #local context state state_descr line
  typeset -A opt_args

  compadd -I '  --   archives a project' 'archive'
    #'delete'   \
    #'help'     \
    #'list'     \
    #'new'      \
    #'open'     \
    #'upgrade'  \
   #  --   deletes a project, use with caution !
   #  --   show help message
   #  --   list active projects
   #  --   creates a project from a recipe
   #  --   initialises the environment for a project
   #  --   upgrades the framework
}
